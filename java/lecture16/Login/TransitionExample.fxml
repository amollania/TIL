<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="mainStack" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lecture17.transition.TransitionExampleController">
   <children>
      <AnchorPane prefHeight="300.0" prefWidth="400.0">
         <children>
            <ImageView fx:id="mainImage" fitHeight="220.0" fitWidth="360.0" layoutX="32.0" layoutY="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@main.jpg" />
               </image>
            </ImageView>
            <Button fx:id="loginButton" layoutX="119.0" layoutY="263.0" mnemonicParsing="false" onAction="#loginAction" text="로그인" />
            <Button layoutX="235.0" layoutY="263.0" mnemonicParsing="false" text="공지사항" />
         </children>
      </AnchorPane>
   </children>
</StackPane>
